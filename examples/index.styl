@import "../index.styl"

windy-reset()

body
  background teal-50
  font-family sans-serif
  p 16

.box
  background white
  shadow()
  rounded()
  p 16

  &_title
    bold()

.animation
  absolute top 100px left 100px
  background red
  windy-transform()
  transform-scale 50
  transform-rotate 90
  animation-bounce()
